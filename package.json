{
  "name": "generate-pi",
  "version": "0.0.0-development",
  "description": "Find PI(π) to the Nth Digit",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run test",
    "watch:test": "npm t -- --watch",
    "test": "mocha src/index.test.js --require @babel/register",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "precommit": "npm run test",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "cover": "nyc --reporter=lcov npm t",
    "commitmsg": "commitlint -e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/palashmon/generate-pi.git"
  },
  "keywords": [
    "generate",
    "pi",
    "π",
    "math",
    "tiny"
  ],
  "author": "Palash Mondal <palashm@live.in>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/palashmon/generate-pi/issues"
  },
  "homepage": "https://github.com/palashmon/generate-pi#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-angular": "^17.4.0",
    "babel-loader": "^8.0.0-beta.0",
    "chai": "^4.1.2",
    "commitizen": "^4.0.0",
    "cz-conventional-changelog": "^3.0.0",
    "husky": "^4.0.2",
    "json-loader": "^0.5.7",
    "mocha": "^7.0.1",
    "npm-run-all": "^4.1.1",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.0",
    "semantic-release": "^17.0.0",
    "webpack": "4.43.0",
    "webpack-cli": "^3.1.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "files": [
    "dist",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 4.x"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "decimal.js": "^10.0.0",
    "is-positive-int": "1.0.2",
    "node-cache": "^5.0.0"
  }
}
